{
    "name": "varenykyphp/locale",
    "description": "VarenykyPHP Locale package",
    "license": "proprietary",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "homepage": "https://www.nopulp.nl/"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "VarenykyLocale\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VarenykyLocale\\Support\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
